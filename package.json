{
  "name": "babel-preset-optimizations",
  "version": "1.7.0",
  "description": "Babel preset for optimizations only babel-minify plugins",
  "keywords": ["babel-preset"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "https://github.com:christophehurpeau/babel-preset-optimizations.git",
  "homepage": "https://github.com/christophehurpeau/babel-preset-optimizations",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "lint": "eslint lib/",
    "preversion": "yarn run lint && repository-check-dirty",
    "release": "repository-check-dirty && pob-release",
    "version": "pob-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "yarn.lock": ["yarn-update-lock", "git add"],
    "{package.json,.eslintrc.json}": [
      "prettier --parser json --write",
      "git add"
    ],
    "lib/**/*.json": ["prettier --parser json --write", "git add"],
    "lib/**/*.js": ["eslint --fix --quiet", "git add"]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "dependencies": {
    "babel-plugin-minify-constant-folding": "^0.5.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.0",
    "babel-plugin-minify-guarded-expressions": "^0.4.3",
    "babel-plugin-minify-simplify": "^0.5.0",
    "babel-plugin-transform-inline-consecutive-adds": "^0.4.3",
    "babel-plugin-transform-undefined-to-void": "^6.9.4"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@pob/eslint-config": "29.0.1",
    "@pob/eslint-config-node": "29.0.1",
    "eslint": "5.15.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "pob-release": "4.8.3",
    "prettier": "1.16.4",
    "repository-check-dirty": "1.0.2",
    "yarn-update-lock": "0.3.3",
    "yarnhook": "0.4.0"
  }
}
